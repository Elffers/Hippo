<h1>Hippop In!</h1>

<%= form_tag signing_in_path do %>
  <div class= "field">
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class= "field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class="actions"><%= submit_tag "Hippop in"%></div?
<% end %>